{
  "name": "@timeplus/vistral",
  "version": "0.1.2",
  "description": "A powerful streaming data visualization library follow grammar of graphics",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": "./dist/styles.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "dev:examples": "vite",
    "build:examples": "vite build -c vite.singlefile.config.ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "visualization",
    "streaming",
    "real-time",
    "charts",
    "antv",
    "g2",
    "react",
    "timeplus",
    "data-visualization",
    "streaming-data"
  ],
  "author": "Timeplus",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/timeplus-io/vistral"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "dependencies": {
    "@antv/g2": "^5.1.0",
    "@antv/s2": "^2.0.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.564.0",
    "ramda": "^0.29.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/lodash": "^4.14.195",
    "@types/ramda": "^0.29.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.26.0",
    "rollup-plugin-dts": "^5.3.1",
    "rollup-plugin-postcss": "^4.0.2",
    "tslib": "^2.6.0",
    "typescript": "^5.1.0",
    "vite": "^5.0.0",
    "vite-plugin-singlefile": "^2.3.0",
    "vitest": "^0.34.0"
  }
}